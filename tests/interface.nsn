type LinesA : Lines {
    lines(text, self: Self) = ["LinesA", text]
}

type LinesB : Lines {
    data: str

    lines(text, self: Self) = ["LinesB", self.data, text]
}

type Lines interface {
    lines(text: str, self: Self): [str]
}

lines_with(lines: Lines) =
    lines.lines("implementation omitted")

a: LinesA = {}
b: LinesB = {data="LinesB 1"}
c: LinesB = {data="LinesB 2"}

main = [
    lines_with(a)
    lines_with(b)
    lines_with(c)
]
